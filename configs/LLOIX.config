<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
</startup>
  <appSettings>
    <!-- General settings-->
    <add key="location" value="LLO Input-X" />
    <add key="twinCat" value="true" />
    <!-- Camera settings-->
    <add key="camera" value="basler" />
    <add key="cameraBitDepth" value="12" />
    <add key="cameraExposureTime" value="500" />
    <add key="cameraWidth" value="4096" />
    <add key="ipAddress" value="10.110.136.137" />
    <add key="frameAverageNum" value="4"/>
    <add key="frameReverse" value="false"/>
    <!-- Graphing settings-->
    <add key="graphingFrameNumber" value="5" />
    <add key="bufferSize" value="20" />
    <add key="numberOfGraphPoints" value="1000" />
    <!-- Email settings-->
    <add key ="emailList" value="mpross2@uw.edu, arnaud.pele@ligo.org"/>
    <!-- Filter settings-->
    <add key ="angleLowPass" value="0.1"/>
    <add key ="angleHighPass" value="0.0005"/>
    <add key ="velocityLowPass" value="0.02"/>
    <add key ="velocityHighPass" value="0.002"/>
    <!-- Fitting settings-->
    <add key ="threshold" value="1200"/>
    <add key ="splitPixel" value="1800"/>
    <add key ="pixelMargin" value="40"/>
    <add key ="patternLength" value="1500"/>
    <!-- Gain settings-->
    <add key ="refGain" value="3499.8"/>
    <add key ="angleGain" value="3645"/>
    <add key ="velGain" value="-5000000000"/>
    <add key ="driftGain" value="60"/>
    <add key ="driftOffset" value="2146"/>
    <add key ="driftOverGain" value="30"/>
    <add key ="driftOverOffset" value="2646"/>
  </appSettings>
</configuration>
